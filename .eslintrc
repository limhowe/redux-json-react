{
  "parser": "babel-eslint",
  "plugins": [
    "import",
    "react"
  ],
  "env": {
    "browser": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:react/recommended"
  ],
  "rules": {
    "array-bracket-spacing": [ 2, "always" ],
    "semi": [ 1, "always"],
    "no-console": 0
  },
  "globals": {
    "__DEV__": true
  }
}

